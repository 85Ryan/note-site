<article class="archive-post archive-type-<%= post.layout %>">
  <div class="archive-post-inner">
    <header class="archive-post-header">
      <%- partial('_post/title', {class_name: 'archive-post-title', index: true, cache: true}) %>
      <%- partial('_post/meta', {class_name: 'archive-post-meta', index: true, cache: true}) %>
    </header>
  </div>
</article>
