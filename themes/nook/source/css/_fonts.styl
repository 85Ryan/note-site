// Fonts: metropolis
@font-face
  font-family metropolis
  font-style normal
  font-weight 100
  src url('/fonts/eot/Metropolis-Thin.eot')
  src local('Metropolis Thin'), url('/fonts/eot/Metropolis-Thin.eot?#iefix') format('embedded-opentype'), url('/fonts/woff/Metropolis-Thin.woff') format('woff'), url('/fonts/TrueType/Metropolis-Thin.ttf') format('truetype')

